<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/gris_claro"
    tools:context=".NuevoUsuarioActivity" >
    
    <TextView
        android:id="@+id/nue_textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="17dp"
        android:text="@string/label_nombre"
        android:textSize="@dimen/tamano_fuente" />

    <EditText
        android:id="@+id/nue_editText1"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="90dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingBottom="@dimen/padding_abajo"
        android:paddingLeft="@dimen/padding_izquierda"
        android:paddingRight="@dimen/padding_derecha"
        android:paddingTop="@dimen/padding_arriba"
        android:textSize="@dimen/tamano_fuente" />

	<TextView
	    android:id="@+id/nue_textView4"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="10dp"
	    android:layout_marginTop="47dp"
	    android:text="@string/label_telefono"
	    android:textSize="@dimen/tamano_fuente" />
	
	<EditText
	    android:id="@+id/nue_editText2"
	    android:layout_width="wrap_content"
	    android:layout_height="32dp"
	    android:layout_alignParentLeft="true"
	    android:layout_alignParentRight="true"
	    android:layout_marginLeft="90dp"
	    android:layout_marginRight="10dp"
	    android:layout_marginTop="40dp"
	    android:ems="10"
	    android:inputType="number"
	    android:paddingBottom="@dimen/padding_abajo"
        android:paddingLeft="@dimen/padding_izquierda"
        android:paddingRight="@dimen/padding_derecha"
        android:paddingTop="@dimen/padding_arriba"
	    android:textSize="@dimen/tamano_fuente" />
	
	<TextView
	    android:id="@+id/nue_textView5"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="10dp"
	    android:layout_marginTop="77dp"
	    android:text="@string/label_email"
	    android:textSize="@dimen/tamano_fuente" />
	
	<EditText
	    android:id="@+id/nue_editText3"
	    android:layout_width="wrap_content"
	    android:layout_height="32dp"
	    android:layout_alignParentLeft="true"
	    android:layout_alignParentRight="true"
	    android:layout_marginLeft="90dp"
	    android:layout_marginRight="10dp"
	    android:layout_marginTop="70dp"
	    android:ems="10"
	    android:inputType="textEmailAddress"
	    android:paddingBottom="@dimen/padding_abajo"
        android:paddingLeft="@dimen/padding_izquierda"
        android:paddingRight="@dimen/padding_derecha"
        android:paddingTop="@dimen/padding_arriba"
	    android:textSize="@dimen/tamano_fuente" />
	
	<TextView
	    android:id="@+id/nue_textView6"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="10dp"
	    android:layout_marginTop="107dp"
	    android:text="@string/label_pais"
	    android:textSize="@dimen/tamano_fuente" />
	
	<EditText
	    android:id="@+id/nue_editText4"
	    android:layout_width="wrap_content"
	    android:layout_height="32dp"
	    android:layout_alignParentLeft="true"
	    android:layout_alignParentRight="true"
	    android:layout_marginLeft="90dp"
	    android:layout_marginRight="10dp"
	    android:layout_marginTop="100dp"
	    android:ems="10"
	    android:inputType="textPersonName"
	    android:paddingBottom="@dimen/padding_abajo"
        android:paddingLeft="@dimen/padding_izquierda"
        android:paddingRight="@dimen/padding_derecha"
        android:paddingTop="@dimen/padding_arriba"
	    android:textSize="@dimen/tamano_fuente" />
	
	<LinearLayout
	    android:id="@+id/linearLayout2"
	    android:layout_width="fill_parent"
	    android:layout_height="50dp"
	    android:layout_alignParentBottom="true"
	    android:orientation="vertical"
	    android:weightSum="1" />
	
</RelativeLayout>